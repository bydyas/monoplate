{
  "name": "@core/swagger",
  "version": "1.0.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "peerDependencies": {
    "@nestjs/common": "11.1.1",
    "@nestjs/swagger": "11.2.5"
  },
  "devDependencies": {
    "@core/eslint-config": "*",
    "@core/typescript-config": "*",
    "@jest/globals": "29.7.0",
    "@nestjs/cli": "11.0.14",
    "@nestjs/schematics": "11.0.9",
    "@types/node": "22.19.3",
    "eslint": "9.39.2",
    "source-map-support": "0.5.21",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.5.4"
  }
}
